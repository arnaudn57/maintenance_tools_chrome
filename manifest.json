{
  "manifest_version": 2,
  "name": "Agence SW Tools",
  "version": "1.0",
  "description": "Extension pour copier la liste de plugins",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
